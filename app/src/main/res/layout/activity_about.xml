        android:id="@+id/KHA_ImageView_layout_about"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:contentDescription="@string/description_imageView_layout_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.77"
        app:layout_constraintHorizontal_bias="0.1"
        app:srcCompat="@drawable/kha" />

    <TextView
        android:id="@+id/KHA_textview_layout_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:text="@string/KHA_textview_layout_about"
        app:layout_constraintStart_toEndOf="@id/KHA_ImageView_layout_about"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.77"
        />

    <View
        style="@style/style_Divider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.82" />

    <TextView
        android:id="@+id/partner_textview_layout_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/partner_textview_layout_about"
        app:layout_constraintBottom_toTopOf="@+id/partner_ImageView_layout_about"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
         />

    <ImageView
        android:id="@+id/partner_ImageView_layout_about"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:contentDescription="@string/description_imageView_layout_home"
