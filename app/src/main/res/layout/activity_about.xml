        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.47"
        app:layout_constraintHorizontal_bias="0.1"
        app:srcCompat="@drawable/mfo" />

    <TextView
        android:id="@+id/MFO_textview_layout_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:text="@string/MFO_textview_layout_about"
        app:layout_constraintStart_toEndOf="@id/MFO_ImageView_layout_about"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.47"
        />

    <ImageView
        android:id="@+id/OCA_ImageView_layout_about"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:contentDescription="@string/description_imageView_layout_home"
