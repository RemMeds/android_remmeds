<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.About">

    <ImageView
        android:id="@+id/logo_ImageView_layout_about"
        android:layout_width="205dp"
        android:layout_height="166dp"
        android:contentDescription="@string/description_imageView_layout_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05"
        app:srcCompat="@drawable/ic_remmeds" />

    <TextView
        android:id="@+id/version_textview_layout_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/version_textview_layout_about"
        app:layout_constraintTop_toBottomOf="@+id/logo_ImageView_layout_about"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />

    <View
        style="@style/style_Divider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35" />

    <TextView
        android:id="@+id/authortitle_textview_layout_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="@string/authortitle_textview_layout_about"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4"
        app:layout_constraintHorizontal_bias="0.2"
        />

    <ImageView
        android:id="@+id/MFO_ImageView_layout_about"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:contentDescription="@string/description_imageView_layout_home"
